<!-- @googlemapUse pgmap_off(); googlemap_on();  googlemap_spot(37xx,12yy); -->
<!-- @use <scene... onstart="googlemap_spot(3x.xx, 12x.xx);" ..> -->
<action name="googlemap_spot">
    set(plugin[googlemap].lat,%1);
    set(plugin[googlemap].lng,%2);
</action>

<!-- @use googlemap_clickActive(sceneName) -->
<action name="googlemap_clickActive">
	plugin[googlemap].activatespot(%1);
	plugin[googlemap].pantospot(%1);
	loadscene(%1);
</action>
<!-- @use spotActive(sceneName) -->
<action name="spotActive">
	plugin[googlemap].activatespot(%1);
</action>

<!-- @pgmapUse googlemap_off(); pgmap_on(mapName); pgmapspot(spotName,radarHeadingValue,mapName);-->

<!-- @use pgmapspot(spotName,radarHeadingValue,mapName) -->
<action name="pgmapspot">
	<!-- off all pgmap radars -->
	set(layer[radar_B1].visible, false);
	set(layer[activespot_B1].visible, false);
	set(layer[radar_B3].visible, false);
	set(layer[activespot_B3].visible, false);
	set(layer[radar_F1].visible, false);
	set(layer[activespot_F1].visible, false);
	set(layer[radar_F2].visible, false);
	set(layer[activespot_F2].visible, false);
	set(layer[radar_F3].visible, false);
	set(layer[activespot_F3].visible, false);
	set(layer[radar_STB1].visible, false);
	set(layer[activespot_STB1].visible, false);
	set(layer[radar_STB2].visible, false);
	set(layer[activespot_STB2].visible, false);
	set(layer[radar_STB3].visible, false);
	set(layer[activespot_STB3].visible, false);
	<!-- copy the spot position to the radar and activespot position -->
	copy(layer[radar_%3].x, layer[%1].x);
	copy(layer[radar_%3].y, layer[%1].y);
	copy(layer[activespot_%3].x, layer[%1].x);
	copy(layer[activespot_%3].y, layer[%1].y);
	<!-- set the radar heading -->
	set(layer[radar_%3].heading, %2);
	<!-- show the radar and the activespot -->
	set(layer[radar_%3].visible, true);
	set(layer[activespot_%3].visible, true);
</action>

<!-- @use pgmap_on(mapName) _B1 B3 F1 F2 F3-->
<action name="pgmap_on">
	set(layer[map_box].visible,true);

	set(layer[map_B1].visible,false);
	set(layer[map_B3].visible,false);
	set(layer[map_F1].visible,false);
	set(layer[map_F2].visible,false);
	set(layer[map_F3].visible,false);
	set(layer[map_STB1].visible,false);
	set(layer[map_STB2].visible,false);
	set(layer[map_STB3].visible,false);

	set(layer[map_%1].visible,true);
</action>
<action name="pgmap_off"> set(layer[map_box].visible,false); </action>

<!-- tour.html html5:"prefer" 일때만 작동 -->
<action name="googlemap_on"> set(plugin[googlemap].visible,true); </action>
<action name="googlemap_off"> set(plugin[googlemap].visible,false); </action>

<action name="openmap">
	if(layer[map_box].status== small, 
		tween(layer[map_box].scale, 1.25);
		tween(layer[map_box].alpha,0.9);
		tween(layer[mapinfo].alpha, 1.25); , 
		tween(layer[map_box].scale, 0.4);
		tween(layer[map_box].alpha,0.8);
		tween(layer[mapinfo].alpha, 0)
	); 
	switch(layer[map_box].status, small, large);
</action>

